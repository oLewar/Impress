    "email": "timur.shemsedinov@gmail.com"
  },
  "main": "./lib/impress",
  "dependencies": {
    "metasync": "0.1.x",
    "globalstorage": "0.1.x",
    "metarhia-jstp": "0.6.x",
    "bcrypt": "1.0.x",
    "colors": "1.1.x",
    "mkdirp": "0.5.x",
    "ncp": "2.0.x",
    "uglify-js": "2.7.x",
    "multiparty": "4.1.x",
    "iconv-lite": "0.4.x",
    "zip-stream": "1.1.x",
    "websocket": "1.0.x",
    "csv": "1.1.x",
    "accept-language": "3.0.x"
  },
  "devDependencies": {
    "eslint": "3.x",
    "eslint-plugin-impress": "1.x"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "readmeFilename": "README.md",
  "scripts": {
    "install": "node install.js",
    "npm": "npm link npm",
    "test": "./tests/unittests.sh && ./tests/integration.sh",
    "lint": "eslint ."
  },
  "jshintConfig": {
    "esversion": 6,
    "predef": ["api", "impress", "global", "window", "navigator", "document", "frames", "escape", "localStorage", "WebSocket", "XMLHttpRequest", "EventSource"],
    "node": true,
    "indent": 2,
    "maxcomplexity": 25
  }
